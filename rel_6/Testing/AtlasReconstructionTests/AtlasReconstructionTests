#!/bin/sh
export AtlasVersion=AtlasReconstruction
alias echoC='echo "$(tput setaf 1)$(tput setab 7) Starting new test... $(tput sgr 0)"'
echoC
athena.py CaloRecEx/CaloRecEx_topOptions.py
echoC
athena.py CaloRecEx/CaloRecEx_topOptions.py
echoC
athena.py InDetExperimentalReco/jobOptions_RecExCommon_masking.py
